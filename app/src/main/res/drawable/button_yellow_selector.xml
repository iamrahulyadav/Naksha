<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#ffffc048" />
            <stroke android:width="1.0px" android:color="#ff999999" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ffffc048" />
            <stroke android:width="1.0px" android:color="#fff77609" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="#ffffc048" />
            <stroke android:width="1.0px" android:color="#fff77609" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ffffe7b4" android:endColor="#ffffc048" android:angle="270.0" />
            <stroke android:width="1.0px" android:color="#ffffbf48" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>